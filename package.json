{
  "name": "@form8ion/shuttle",
  "description": "A utility for moving a repository from one account to another",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/shuttle",
  "bugs": "https://github.com/form8ion/shuttle/issues",
  "homepage": "https://npm.im/@form8ion/shuttle",
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib/index.mjs"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "sideEffects": false,
  "scripts": {
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "generate:md": "remark . --output",
    "test:unit:base": "DEBUG=any vitest run",
    "lint:peer": "npm ls >/dev/null",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "pretest:integration:base": "run-s build",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "lint:publish": "publint --strict"
  },
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@travi/cli-messages": "^1.0.4",
    "remark": "^12.0.0"
  },
  "devDependencies": {
    "@babel/register": "^7.8.6",
    "@form8ion/babel-preset": "1.6.110",
    "@form8ion/commitlint-config": "^1.0.9",
    "@form8ion/eslint-config": "^1.6.14",
    "@form8ion/eslint-config-cucumber": "^1.4.0",
    "@form8ion/remark-lint-preset": "6.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@travi/any": "^2.0.7",
    "ban-sensitive-files": "^1.9.2",
    "c8": "8.0.1",
    "cross-env": "7.0.3",
    "cucumber": "^6.0.5",
    "cz-conventional-changelog": "^3.1.0",
    "gherkin-lint": "^4.0.1",
    "husky": "^4.2.3",
    "jest-when": "3.6.0",
    "lockfile-lint": "4.12.1",
    "mock-fs": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "publint": "0.2.2",
    "remark-cli": "^7.0.1",
    "remark-toc": "^7.0.0",
    "remark-usage": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "vitest": "0.34.3"
  }
}
