{
  "name": "@form8ion/shuttle",
  "description": "A utility for moving a repository from one account to another",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "form8ion/shuttle",
  "bugs": "https://github.com/form8ion/shuttle/issues",
  "homepage": "https://npm.im/@form8ion/shuttle",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "@babel/register": "^7.8.6",
    "@form8ion/babel-preset": "^1.6.14",
    "@form8ion/commitlint-config": "^1.0.9",
    "@form8ion/eslint-config": "^1.6.14",
    "@form8ion/eslint-config-mocha": "^1.2.7",
    "@form8ion/remark-lint-preset": "^1.0.2",
    "@travi/any": "^2.0.7",
    "ban-sensitive-files": "^1.9.2",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "lockfile-lint": "^4.0.0",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "remark-cli": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "sinon": "^9.0.0",
    "travis-lint": "^1.0.0"
  }
}
